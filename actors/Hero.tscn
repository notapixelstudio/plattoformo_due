script/source = "extends Node2D

func show_state(name : String):
	for child in get_children():
		child.visible = false
		
	match name:
		'on_floor':
			$OnFloor.visible = true
		'on_wall_left':
			$OnWallLeft.visible = true
		'on_wall_right':
			$OnWallRight.visible = true
"

